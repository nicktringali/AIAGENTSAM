# API Keys (REQUIRED)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model Configuration
PLANNER_MODEL=gpt-4-0125-preview
LOCATOR_MODEL=gpt-4-0125-preview
CODER_MODEL=claude-3-opus-20240229
CRITIC_MODEL=gpt-4-0125-preview
REVIEWER_MODEL=claude-3-opus-20240229

# Feature Flags
ENABLE_MEMORY=true
ENABLE_CRITIC=true
ENABLE_REVIEWER=true
ENABLE_MONITORING=true

# Team Configuration
COORDINATION_MODE=swarm
MAX_ITERATIONS=5
AGENT_TIMEOUT=300

# Docker Sandbox Settings
SANDBOX_MEMORY_LIMIT=512m
SANDBOX_CPU_LIMIT=1.0
SANDBOX_TIMEOUT=120
SANDBOX_MAX_PIDS=100

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_secure_redis_password_here
REDIS_DB=0

# ChromaDB Configuration
CHROMA_HOST=chromadb
CHROMA_PORT=8001
CHROMA_PERSIST_DIRECTORY=/data/chroma

# API Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_RELOAD=false
LOG_LEVEL=INFO
CORS_ORIGINS=["https://your-domain.com"]

# Security Settings
SECRET_KEY=your_very_secure_secret_key_here
API_KEY=your_api_authentication_key_here
RATE_LIMIT=100
RATE_LIMIT_PERIOD=3600

# Monitoring
PROMETHEUS_PORT=9091
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_here

# Deployment Environment
ENVIRONMENT=production
DEBUG=false